{
   "version": 2,
   "name": "music-album",
   "builds": [
     {
       "src": "index.js",
       "use": "@vercel/node"
     }
   ],
   "routes": [
     {
       "src": "/api/contact",
       "dest": "index.js"
     },
     {
       "src": "/(.*)",
       "dest": "/index.js"
     }
   ],
   "rewrites": [
     { "source": "/(.*)", "destination": "/index.html" }
   ]
 }
 